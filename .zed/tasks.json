    "use_new_terminal": true,
    "allow_concurrent_runs": false,
    "reveal": "always",
    "hide": "on_success"
  },
  {
    "label": "nuxt generate",
    "command": "cd \"${ZED_WORKTREE_ROOT}/apps/frontend\" && pnpm generate",
    "use_new_terminal": true,
    "allow_concurrent_runs": false,
    "reveal": "always",
    "hide": "on_success"
  },
  {
    "label": "npx serve",
    "command": "npx serve \"${ZED_WORKTREE_ROOT}/.output/public\"",
    "use_new_terminal": true,
    "allow_concurrent_runs": false,
    "reveal": "always",
    "hide": "on_success"
  },
  {
    "label": "cargo run",
    "command": "cd \"${ZED_WORKTREE_ROOT}\" && cargo run",
    "use_new_terminal": true,
    "allow_concurrent_runs": false,
    "reveal": "always",
